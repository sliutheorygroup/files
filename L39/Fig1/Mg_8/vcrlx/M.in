&control
  calculation ='vc-relax'
  restart_mode ='from_scratch',
  prefix       ='ZnO',
  outdir='./temp'
  pseudo_dir='./GBRV_PBEsol'
  disk_io='low'
  etot_conv_thr=1.D-7,
  forc_conv_thr=1.D-6,
  verbosity    ='high',
  tprnfor=.t.
  tstress=.t.
  nstep=200
!  tefield = .true.
!  dipfield = .true.

 /
&system
  nosym = .true.,
  ibrav= 0,
  nat=  72,
  ntyp= 3,
  tot_charge = 0.0
  ecutwfc =  60,
  ecutrho = 500,
  occupations='smearing', smearing='mv', degauss=0.001,
!  eamp = 0.0
!  edir = 3
!  emaxpos = 0.1
!  eopreg = 0.05

